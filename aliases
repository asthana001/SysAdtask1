#!/bin/bash

echo alias sudo='sudo ' >> ~/.bashrc
echo alias userGenerate='./createuser' >> ~/.bashrc
echo alias permit='./permission' >> ~/.bashrc
echo alias autoSchedule='./sch' >> ~/.bashrc
echo alias attendance='./attd' >> ~/.bashrc
echo alias record='./rcd' >> ~/.bashrc
echo alias finalattendance='./fattd' >> ~/.bashrc
echo alias nearest='./near' >> ~/.bashrc

#source ~/.bashrc
echo Aliases created
